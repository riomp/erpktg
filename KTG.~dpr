{$R *.res}

begin
  {$DEFINE OLDU}
  {$IFDEF OLDU}
  Application.Initialize;
  Application.CreateForm(Tdm, dm);
  Application.CreateForm(TfrmUtama, frmUtama);
  Application.CreateForm(TfrmLogin, frmLogin);
  Application.CreateForm(TfrmPenerimaanBarang, frmPenerimaanBarang);
  Application.CreateForm(TfrmPilihPO, frmPilihPO);
  frmUtama.Show;

  frmLogin.ShowModal;

  if frmLogin.ModalResult = mrOk then begin
    frmLogin.Close;
    frmLogin.Destroy;
  end;

  Application.Run;
  {$ELSE}
  Application.Initialize;
  Application.CreateForm(Tdm, dm);
  Application.CreateForm(TfrmUtama, frmUtama);
  Application.CreateForm(TfrmKetUpdate_NEW, frmKetUpdate_NEW);
  if frmKetUpdate_NEW.ShowModal = mrOk then begin
    frmKetUpdate_NEW.Release;
    Application.Terminate;
  end
  else begin
    frmKetUpdate_NEW.Release;
    Application.CreateForm(TfrmLogin, frmLogin);
    frmUtama.Show;

    frmLogin.ShowModal;

    if frmLogin.ModalResult = mrOk then begin
      frmLogin.Close;
      frmLogin.Destroy;
    end;

  end;
  Application.Run;
  {$ENDIF}
end.
